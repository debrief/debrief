//first check wether sampledata.dpf file exist in navigator view
get-view Navigator | get-tree | get-item -path "sample_data/sample.dpf" | get-property "getText()" 
    | equals "sample.dpf" | verify-true
 //open it   
with [get-view Navigator | get-tree] {
    select "sample_data/sample.dpf" | double-click
}
wait 500
