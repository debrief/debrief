<ScenarioController OutputDirectory="MultiStatics/Output"
	xmlns="http://www.mwc.org/asset">
	<ScenarioGenerator>
	</ScenarioGenerator>
	<ObserverList>
		<TimeObserver Name="elapsed time" Active="true">
			<Duration Value="45" Units="minutes" />
		</TimeObserver>
		<DebriefReplayObserver Name="DebriefPlot"
			record_detections="true" record_positions="true" Active="true"
			record_decisions="false" file_name="multi_statics">
		</DebriefReplayObserver>
	</ObserverList>
</ScenarioController>

