					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="OnTop"/>
							<xs:enumeration value="DirectedOnTop"/>
							<xs:enumeration value="MakeWaypoint"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
