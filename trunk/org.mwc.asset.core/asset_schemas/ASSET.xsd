						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="DetectionLevel" use="optional">
					<xs:annotation>
						<xs:documentation>The level of identification/classification required to
							count as detected</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:NMTOKEN">
							<xs:enumeration value="Undetected"/>
							<xs:enumeration value="Detected"/>
							<xs:enumeration value="Classified"/>
							<xs:enumeration value="Identified"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CoreFileObserverType">
